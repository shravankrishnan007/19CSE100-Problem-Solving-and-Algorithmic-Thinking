    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="TO FIND THE REMAINDER WHEN DIVIDING TWO NUMBERS"/>
            <declare name="DIV" type="Integer" array="False" size=""/>
            <declare name="DIVISOR" type="Integer" array="False" size=""/>
            <declare name="Q" type="Integer" array="False" size=""/>
            <declare name="R" type="Integer" array="False" size=""/>
            <output expression="&quot;ENTER THE NUMBER YOU WANT TO DIVIDE:&quot;" newline="True"/>
            <input variable="DIV"/>
            <output expression="&quot;ENTER THE NUMBER WHICH YOU WANT TO DIVIDE WITH:&quot;" newline="True"/>
            <input variable="DIVISOR"/>
