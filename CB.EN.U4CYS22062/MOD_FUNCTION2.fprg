            <assign variable="R" expression="DIV-DIVISOR*Q"/>
            <output expression="&quot;THE REMAINDER IS :&quot; &amp;R" newline="True"/>
            <output expression="&quot;THE QUOTIENT IS:&quot; &amp;Q" newline="True"/>
